				</LinearLayout>
				</FrameLayout>
                <FrameLayout android:id="@+id/FrameLayout03" android:layout_weight="1" android:layout_width="fill_parent" android:layout_height="wrap_content">
                    
<LinearLayout android:id="@+id/LinearLayout08" android:layout_height="fill_parent" android:layout_width="fill_parent" android:orientation="vertical">
<TextView android:text="@+id/HourlyText_2_1" android:id="@+id/HourlyText_2_1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="20px" android:layout_gravity="center"></TextView>
