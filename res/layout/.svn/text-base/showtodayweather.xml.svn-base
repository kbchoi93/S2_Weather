    <TextView android:text="" android:id="@+id/TextView04" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="40px"></TextView>
    <TextView android:text="/" android:id="@+id/TextView05" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="32px"></TextView>
    <TextView android:text="" android:id="@+id/TextView06" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="50px"></TextView>
    <TextView android:text="" android:id="@+id/TextView07" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="40px"></TextView>
</LinearLayout>
<LinearLayout android:id="@+id/LinearLayout06" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <TextView android:text="" android:id="@+id/TextView03" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="22px"  android:layout_marginLeft="17dip"></TextView>
    <TextView android:text="" android:id="@+id/TextView09" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="16px"></TextView>
    <TextView android:text="/" android:id="@+id/TextView10" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="16px"></TextView>
    <TextView android:text="" android:id="@+id/TextView11" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="22px"></TextView>
    <TextView android:text="" android:id="@+id/TextView12" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="16px"></TextView>
</LinearLayout>



</LinearLayout>

</FrameLayout>
    <RelativeLayout android:id="@+id/RelativeLayout02" android:layout_height="wrap_content" android:layout_gravity="bottom" android:layout_width="fill_parent">
        <FrameLayout android:id="@+id/FrameLayout07" android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_above="@+id/ImageView03">
	  <ImageView android:id="@+id/ImageView03" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/tab_lighting_left_ss" android:layout_gravity="left|center_vertical"/>
	  <ImageView android:id="@+id/ImageView04" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/tab_lighting_right_ss" android:layout_gravity="right|center_vertical"/>
	  <HorizontalScrollView android:id="@+id/HorizontalScrollView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scrollbars="none">
	    <ImageView android:id="@+id/showtoday_week" android:layout_height="fill_parent" android:layout_width="fill_parent"/>
	  </HorizontalScrollView>
        </FrameLayout>
    </RelativeLayout>
    <include layout="@layout/bottom_bar" />
</LinearLayout>
