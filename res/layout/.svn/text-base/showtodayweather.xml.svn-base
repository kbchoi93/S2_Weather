</LinearLayout>
</FrameLayout>
                <FrameLayout android:id="@+id/FrameLayout04" android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_weight="1">
                    
<LinearLayout android:id="@+id/LinearLayout09" android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="vertical">
<TextView android:text="@+id/HourlyText_3_1" android:id="@+id/HourlyText_3_1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="20px" android:layout_gravity="center"></TextView>
<ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/Hourly_3" android:layout_gravity="center_vertical|center_horizontal"></ImageView>
<TextView android:text="@+id/HourlyText_3_2" android:id="@+id/HourlyText_3_2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="18px" android:layout_gravity="center"></TextView>
</LinearLayout>

</FrameLayout>
                <FrameLayout android:id="@+id/FrameLayout05" android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_weight="1">
                    
<LinearLayout android:id="@+id/LinearLayout10" android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="vertical">
<TextView android:text="@+id/HourlyText_4_1" android:id="@+id/HourlyText_4_1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="20px" android:layout_gravity="center"></TextView>
<ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/Hourly_4" android:layout_gravity="center_vertical|center_horizontal"></ImageView>
<TextView android:text="@+id/HourlyText_4_2" android:id="@+id/HourlyText_4_2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="18px" android:layout_gravity="center"></TextView>


</LinearLayout>
</FrameLayout>
            </LinearLayout>
        




</LinearLayout>
        
<LinearLayout android:id="@+id/LinearLayout02" android:layout_height="wrap_content" android:orientation="vertical" android:layout_width="fill_parent">
<LinearLayout android:id="@+id/LinearLayout05" android:layout_width="fill_parent" android:layout_height="wrap_content" android:background="@drawable/location_name_bg_01">
    <TextView android:text="" android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="40px" android:layout_marginLeft="17dip" android:layout_marginTop="5dip" android:layout_gravity="bottom"></TextView>
    <TextView android:text="" android:id="@+id/TextView08" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="22px" android:layout_gravity="bottom"></TextView>
</LinearLayout>
<LinearLayout android:id="@+id/LinearLayout04" android:layout_width="wrap_content" android:layout_height="wrap_content">
