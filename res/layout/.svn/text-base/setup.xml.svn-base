<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <TextView 
        android:layout_width="fill_parent"
        android:layout_height="38px"
        android:text="설정"
        android:gravity="center_vertical"
        android:textColor="#ffffff"
        android:background="@drawable/activity_title_bar"
        />
    <TextView 
        android:layout_width="fill_parent"
        android:layout_height="38px"
        android:text="날짜보기"
        android:background="@drawable/list_title_bg"
        android:gravity="center_vertical"
        android:textColor="#ffffff"
        android:paddingLeft="10px"
        />
    <ImageView
        android:id="@+id/setupbtn1"
        android:layout_width="fill_parent"
        android:layout_height="95px"
        android:clickable="true"
        android:background="@drawable/list_view_selector"
        />
    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#8c8c8c"
        />
    <ImageView
        android:id="@+id/setupbtn2"
        android:layout_width="fill_parent"
        android:layout_height="95px"
        android:clickable="true"
        android:background="@drawable/list_view_selector"
        />
    <TextView 
        android:layout_width="fill_parent"
        android:layout_height="38px"
        android:text="시간보기"
        android:background="@drawable/list_title_bg"
        android:gravity="center_vertical"
        android:textColor="#ffffff"
        android:paddingLeft="10px"
        />
    <ImageView
        android:id="@+id/setupbtn3"
        android:layout_width="fill_parent"
        android:layout_height="95px"
        android:clickable="true"
        android:background="@drawable/list_view_selector"
        />
    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#8c8c8c"
        />
    <ImageView
        android:id="@+id/setupbtn4"
        android:layout_width="fill_parent"
        android:layout_height="95px"
        android:clickable="true"
        android:background="@drawable/list_view_selector"
        />
    <TextView 
        android:layout_width="fill_parent"
        android:layout_height="38px"
        android:text="온도표시"
        android:background="@drawable/list_title_bg"
        android:gravity="center_vertical"
        android:textColor="#ffffff"
        android:paddingLeft="10px"
        />
    <ImageView
        android:id="@+id/setupbtn5"
        android:layout_width="fill_parent"
        android:layout_height="95px"
        android:clickable="true"
        android:background="@drawable/list_view_selector"
        />
    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#8c8c8c"
        />
    <ImageView
        android:id="@+id/setupbtn6"
        android:layout_width="fill_parent"
        android:layout_height="95px"
        android:clickable="true"
        android:background="@drawable/list_view_selector"
        />
    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#8c8c8c"
        />
</LinearLayout>
