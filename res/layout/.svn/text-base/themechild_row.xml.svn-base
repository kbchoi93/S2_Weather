<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="92px"
    android:background="@drawable/list_view_selector"
    >
    
    <LinearLayout
        android:orientation="vertical"
        android:layout_marginLeft="50px"
        android:layout_width="365px"
        android:layout_height="92px"
        >

    <TextView android:id="@+id/childname"
        android:layout_marginTop="12px"
        android:focusable="false"
        android:textSize="30px"
        android:layout_width="fill_parent"
        android:gravity="left|center_vertical"
        android:layout_height="40px"
        android:textColor="#ffffff"
        />

    <TextView android:id="@+id/childaddress"
         android:focusable="false"
         android:textSize="20px"
         android:layout_width="fill_parent"
         android:layout_height="47px"
         android:layout_marginTop="2px"
         android:textColor="#999999"
         />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="57px"
        android:layout_height="92px"
        android:gravity="center_vertical"
        >
    <CheckBox android:id="@+id/check1"
           android:focusable="false"
           android:clickable="false"
           android:checkMark="@drawable/checkbtn_selector"
           android:layout_width="0px"
           android:layout_height="0px"
           />
    <ImageView
        android:id="@+id/checkimg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_check_off"
        />
    </LinearLayout>
</LinearLayout>
