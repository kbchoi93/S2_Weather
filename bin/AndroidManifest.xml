<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.weathernews.Weather" 
      android:versionCode="10" android:versionName="1.9">
    <application android:icon="@drawable/icon" android:label="@string/app_name"
    	android:debuggable="false" android:largeHeap="true">
    	<uses-library android:required="true" android:name="com.google.android.maps"></uses-library>
        <activity android:name=".Startup"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait"
		  >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TabRoot" android:label="@string/app_name" android:screenOrientation="portrait"  android:configChanges="keyboardHidden|orientation"/>
        <activity android:name=".TabRootHorizontal" android:label="@string/app_name" android:launchMode="singleTask" />
        <activity android:name=".Weather" android:label="@string/app_name" android:screenOrientation="portrait" />
        <activity android:name=".Setup" android:label="@string/app_name" android:screenOrientation="portrait" />
	<activity android:name=".showTodayWeather" android:label="@string/app_name" />
	<activity android:name=".DndActivity" android:label="@string/app_name" android:screenOrientation="portrait" android:configChanges="keyboardHidden|orientation" />
	<activity android:name=".ThemeList" android:label="@string/app_name" />
	<activity android:name=".TodayThemeWeather" android:label="@string/app_name"/>
	<activity android:name=".LiveCam" android:label="@string/app_name" android:screenOrientation="portrait" />
	<activity android:name=".ViewLiveCam" android:label="@string/app_name" android:screenOrientation="portrait" />
	<activity android:name=".SearchCity" android:label="@string/app_name" android:screenOrientation="portrait" />
	<activity android:name=".CityGroup" android:label="@string/app_name" android:screenOrientation="portrait" />
	<activity android:name=".CityGroupList" android:label="@string/app_name" android:screenOrientation="portrait" />
	<activity android:name=".SearchTheme" android:label="@string/app_name" android:screenOrientation="portrait" />
	<activity android:name=".ThemeGroup" android:label="@string/app_name" android:screenOrientation="portrait" />
	<activity android:name=".CustomDialog" android:launchMode="standard" android:screenOrientation="portrait" android:theme="@android:style/Theme.Translucent"/>
	<activity android:name=".Land48h" android:label="@string/app_name"/>
	<activity android:name=".Land48hGraph" android:label="@string/app_name"/>
	<activity android:name=".Land10days" android:label="@string/app_name"/>
	<activity android:name=".Land10daysGraph" android:label="@string/app_name"/>
	<activity android:name=".LandRadar" android:label="@string/app_name"/>
	<activity android:name=".LandSattle" android:label="@string/app_name"/>
	<activity android:name=".MapTest" android:label="@string/app_name" />
        <activity android:name=".WeatherPopUp" android:label="@string/app_name" android:theme="@android:style/Theme.Translucent" android:screenOrientation="sensor"/>
        <activity android:name=".DeleteWeatherList" android:label="@string/app_name" android:screenOrientation="portrait" />
        <activity android:name=".DndActivityTheme" android:label="@string/app_name" android:screenOrientation="portrait" android:configChanges="keyboardHidden|orientation" />
        <activity android:name=".DeleteThemeList" android:label="@string/app_name" android:screenOrientation="portrait" />

    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_GPS"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.RESTART_PACKAGES"></uses-permission>
    <uses-sdk android:minSdkVersion="10" 
        android:targetSdkVersion="14"/>

<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
</manifest>
